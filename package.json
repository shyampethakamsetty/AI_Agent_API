{
  "name": "ai-agent-server",
  "version": "1.0.0",
  "description": "A pluggable AI agent server with RAG, memory, and plugin system",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "rag",
    "openai",
    "typescript",
    "express"
  ],
  "author": "Shyam Pethakamsetty",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "mathjs": "^12.2.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0"
  }
} 